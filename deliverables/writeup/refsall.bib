@article{efstathiou_cobe_1992,
	title = {{COBE} background radiation anisotropies and large-scale structure in the universe},
	volume = {258},
	issn = {0035-8711},
	url = {http://adsabs.harvard.edu/abs/1992MNRAS.258P...1E},
	doi = {10.1093/mnras/258.1.1P},
	abstract = {We discuss the constraints imposed on theoretical models by the COBE 
measurements of the microwave background anisotropies by observations of
galaxy clustering, and by the observed streaming motions of galaxies.
When normalized to match the COBE results, models with Omega = 1 and
with more large-scale power than the standard cold dark matter (CDM)
model predict lower streaming motions than are observed, but agree well
with the dynamics of clustering on smaller scales. Unbiased Omega = 1
CDM models fit the COBE data and the streaming motions, but are less
easily reconciled with galaxy clustering data on either small or large
scales. Spatially flat CDM models with Omega of about 0.2 and a
cosmological constant require the mass to be substantially more
clustered than the galaxies in order to be consistent with COBE and with
observed streaming motions. They are then in conflict, however, with
dynamical measurements on smaller scales.},
	urldate = {2021-04-28},
	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Efstathiou, G. and Bond, J. R. and White, S. D. M.},
	month = sep,
	year = {1992},
	keywords = {Anisotropic Media, Astronomical Models, Background Radiation, Cosmic Background Explorer Satellite, Dark Matter, Galactic Clusters, Relic Radiation, Universe},
	pages = {1P--6P},
	file = {Full Text PDF:/Users/connor/Zotero/storage/EU8V62VA/Efstathiou et al. - 1992 - COBE background radiation anisotropies and large-s.pdf:application/pdf}
}

