	author = {Belokurov, V. and Koposov, S. E. and Evans, N. W. and Peñarrubia, J. and Irwin, M. J. and Smith, M. C. and Lewis, G. F. and Gieles, M. and Wilkinson, M. I. and Gilmore, G. and Olszewski, E. W. and Niederste-Ostholt, M.},
	month = jan,
	year = {2014},
	keywords = {Galaxy: kinematics and dynamics, Galaxy: halo, blue stragglers, Galaxy: fundamental parameters, stars: carbon, stars: horizontal branch},
	pages = {116--131},
	file = {Full Text PDF:/Users/connor/Zotero/storage/ZEUEJPGF/Belokurov et al. - 2014 - Precession of the Sagittarius stream.pdf:application/pdf}
}

