	author = {Robles, Victor H. and Bullock, James S. and Elbert, Oliver D. and Fitts, Alex and González-Samaniego, Alejandro and Boylan-Kolchin, Michael and Hopkins, Philip F. and Faucher-Giguère, Claude-André and Kereš, Dušan and Hayward, Christopher C.},
