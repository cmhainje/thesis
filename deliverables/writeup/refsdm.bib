	note = {arXiv: astro-ph/9611107},
	keywords = {Astrophysics},
	file = {arXiv Fulltext PDF:/Users/connor/Zotero/storage/RG3GPTH4/Navarro et al. - 1997 - A Universal Density Profile from Hierarchical Clus.pdf:application/pdf;arXiv.org Snapshot:/Users/connor/Zotero/storage/6DIA7JTG/9611107.html:text/html}
}

