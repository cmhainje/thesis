	journal = {Monthly Notices of the Royal Astronomical Society},
	author = {Chan, T. K. and Kereš, D. and Oñorbe, J. and Hopkins, P. F. and Muratov, A. L. and Faucher-Giguère, C.-A. and Quataert, E.},
