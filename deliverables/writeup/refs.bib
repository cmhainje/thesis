	pages = {L143--L146},
	file = {Full Text PDF:/Users/connor/Zotero/storage/L9V8AG4C/Burkert - 2000 - The Structure and Evolution of Weakly Self-interac.pdf:application/pdf}
}

@article{yoshida_weakly_2000,
	title = {Weakly {Self}-interacting {Dark} {Matter} and the {Structure} of {Dark} {Halos}},
